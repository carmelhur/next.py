class Octopus:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def birthday(self):
        self.age += 1

    def get_age(self):
        return self.age

octopus1 = Octopus("Octavio", 3)
octopus2 = Octopus("Inky", 5)

octopus1.birthday()

print(octopus1.get_age())
print(octopus2.get_age())
